    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@hookform/resolvers": "^2.9.8",
    "@mui/icons-material": "^5.10.3",
    "@mui/lab": "^5.0.0-alpha.99",
    "@mui/material": "^5.10.5",
    "@mui/x-date-pickers": "^5.0.3",
    "@reduxjs/toolkit": "^1.8.5",
    "cloudinary-build-url": "^0.2.4",
    "date-fns": "^2.29.3",
    "intersection-observer": "^0.12.2",
    "isomorphic-fetch": "^3.0.0",
    "just-debounce-it": "^3.1.1",
    "next": "12.3.0",
    "notistack": "^2.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.35.0",
    "react-redux": "^8.0.2",
    "redux-persist": "^6.0.0",
    "sharp": "^0.31.1",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@cld-apis/types": "^0.1.6",
    "@storybook/addon-actions": "^6.5.11",
    "@storybook/addon-essentials": "^6.5.11",
    "@storybook/addon-interactions": "^6.5.11",
    "@storybook/addon-links": "^6.5.11",
    "@storybook/builder-webpack5": "^6.5.11",
    "@storybook/manager-webpack5": "^6.5.11",
    "@storybook/react": "^6.5.11",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jest": "^29.0.1",
    "@types/node": "18.7.17",
    "@types/react": "^18.0.19",
    "@types/react-dom": "18.0.6",
    "@types/react-redux": "^7.1.24",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "babel-loader": "^8.2.5",
    "cypress": "^10.7.0",
    "eslint": "^8.25.0",
    "eslint-config-next": "12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-storybook": "^0.6.4",
    "husky": "^8.0.1",
